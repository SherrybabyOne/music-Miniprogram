.modal {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
}
.panel {
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  min-height: 300rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
}
/* 可以对外部样式进行修改 */
.modal .icon-shanchu1 {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  padding: 20rpx;
}